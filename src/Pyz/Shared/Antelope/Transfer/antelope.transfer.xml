<?xml version="1.0"?>
<transfers xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns="spryker:transfer-01"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="Antelope">
        <property name="idAntelope" type="int"/>
        <property name="name" type="string"/>
        <property name="color" type="string" />
        <property name="age" type="int"/>
        <property name="locationId" type="int" />
        <property name="typeId" type="int" />
        <property name="weight" type="float" />
        <property name="gender" type="string" />
        <property name="type" type="AntelopeType" />
        <property name="location" type="AntelopeLocation" />
    </transfer>
    <transfer name="AntelopeCriteria">
        <property name="name" type="string"/>
    </transfer>
    <transfer name="AntelopeResponse">
        <property name="antelope" type="Antelope"/>
        <property name="isSuccessFul" type="bool"/>
    </transfer>
    <transfer name="AntelopeCollection">
        <property name="antelopes" type="Antelope[]" singular="antelope"/>
    </transfer>
    <transfer name="AntelopeCondition">
        <property name="idAntelope" type="int" />
        <property name="name" type="string" />
        <property name="color" type="string" />
        <property name="locationId" type="int" />
        <property name="typeId" type="int" />
        <property name="antelopeIds" type="int[]" singular="idAntelope" />
    </transfer>
    <transfer name="AntelopeSort">
        <property name="field" type="string" />
        <property name="isAscending" type="boolean" />
    </transfer>
    <transfer name="AntelopeCriteria">
        <property name="antelopeConditions" type="AntelopeCondition" />
        <property name="sortCollection" type="AntelopeSort[]" />
        <property name="pagination" type="PaginationTransfer" />
    </transfer>
</transfers>
