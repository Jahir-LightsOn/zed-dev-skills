{% extends '@Gui/Layout/layout.twig' %}

{% set widget_title = 'Create Antelope Location' %}

{% block action %}
    {{ backActionButton(backUrl, 'Back to antelope locations list' | trans) }}
{% endblock %}

{% block content %}
    {% embed '@Gui/Partials/widget.twig' %}
        {% block widget_content %}
            {{ form_start(form) }}

            {% for field in form.children %}
                {{ form_row(field) }}
            {% endfor %}

            <input type="submit" class="btn btn-primary safe-submit"
                   value="{{ 'Create' | trans }}"/>

            {{ form_end(form) }}
        {% endblock %}
    {% endembed %}
{% endblock %}
